<div class="row">
    <div class="col-md-8 col-md-offser-2">
        <h1>User Profile</h1>
        <hr>
        <h2>
            My Orders
        </h2>
        {{# each orders}}
        <div class="panel panel-default">
            <div class="panel-body">
                <ul class="list-group">
                    {{# each this.items}}
                    <li class="this-group-item">
                        <span class="badge">Giá: {{this.price}}VNĐ</span>
                        | {{this.item.name}} | Số lượng:{{this.qty}} Units
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="panel-footer">
                <strong>Total: {{this.cart.totalPrice}}</strong>
            </div>
            {{/each}}
        </div>
</div>
</div>